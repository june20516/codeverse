{
  "name": "codeverse",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "server": "next build && npx http-server out",
    "deploy": "rm -rf node_modules/.cache && next build && touch out/.nojekyll && git add ./out && git commit -m \"[publish]$(date +\"%H:%M, %d/%m/%Y\")\" && git subtree push --prefix out gh-page main;",
    "deployed-pull": "git subtree pull --prefix out gh-page main;",
    "write": "node commands/write.js"
  },
  "dependencies": {
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "gray-matter": "^4.0.3",
    "next": "13.4.4",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-add-classes": "^1.0.0",
    "rehype-external-links": "^2.1.0",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.2",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.4",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "prettier": "^2.8.8"
  }
}
