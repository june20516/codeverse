{
  "name": "codeverse",
  "version": "0.2.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "server": "next build && npx http-server out",
    "deploy-init": "git remote add gh-page https://github.com/june20516/june20516.github.io.git",
		"deploy": "git pull origin main && rm -rf node_modules/.cache && next build && touch out/.nojekyll && git add ./out && git commit -m \"[publish]$(date +\"%H:%M, %d/%m/%Y\")\" && git subtree push --prefix out gh-page main; && git push origin main",
    "write": "node commands/write.js"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.0.2",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "eslint": "^9.10.0",
    "eslint-config-next": "13.4.4",
    "gray-matter": "^4.0.3",
    "next": "13.4.4",
    "postcss": "8.4.24",
    "postcss-nested": "^6.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-add-classes": "^1.0.0",
    "rehype-external-links": "^2.1.0",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.2",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "typescript": "5.0.4",
    "unified": "^10.1.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.12",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^2.8.8",
    "typescript-eslint": "^8.5.0"
  }
